<br>


<div class="container">
	<% @posts.each do |post| %>
	 		<div class="col-md-10">
		        <h2><%= post.title %></h2>
		        By <h7><%= post.author %></h7> at <h7><%= post.published_at %></h7>
		        <p id="paragraph"><%= simple_format post.content %></p>
		    </div>
		      	
		    <div class="col-md-2">
		      	<p class="output">Right column 3</p>
		    </div>
		<% end %>
</div>

<button class="btn btn-primary" id="whatever" type="submit"">Search</button>

<script type="text/javascript">

$(document).ready(function(){
	$("#whatever").click(function() {
		texty = $('p').text();
		console.log(texty)
		if(texty.contains("Tulane")){
			$(".output").replaceWith("New text goes here"); 
			} else{
				console.log("nothing")
			}; 
	}); 
});
</script>


