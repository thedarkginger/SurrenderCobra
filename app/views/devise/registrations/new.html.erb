<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>


 <div class="form-group">
  <div><%= f.label :email %><br />
  <%= f.email_field :email, "form-control", autofocus: true %></div>
 </div>

 <div class="form-group">
  <div><%= f.label :password %><br />
    <%= f.password_field :password, "form-control", autocomplete: "off" %></div>
 </div>

 <div class="form-group">
  <div><%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, "form-control", autocomplete: "off" %></div>
  </div>

  <div><%= f.submit "Sign up", class: "btn btn-primary" %></div>
<% end %>

<%= render "devise/shared/links" %>




   